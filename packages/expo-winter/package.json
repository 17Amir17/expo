{
  "name": "expo-winter",
  "version": "1.0.0",
  "description": "Native WinterCG runtime for Expo apps.",
  "main": "build/winter",
  "sideEffects": false,
  
  "scripts": {
    "build:android": "metro build ./src/winter.ts -O build/winter.android.js -p android --minify",
    "build:ios": "metro build ./src/winter.ts -O build/winter.ios.js -p ios --minify && cp build/winter.ios.js ios/js/winter.js && npm run hbc:ios",
    "hbc:ios": "node scripts/to-hermes-ios.js",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "keywords": [
    "react-native",
    "expo",
    "winter-cg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo.git",
    "directory": "packages/expo-winter"
  },
  "bugs": {
    "url": "https://github.com/expo/expo/issues"
  },
  "author": "650 Industries, Inc.",
  "license": "MIT",
  "homepage": "https://docs.expo.dev/",
  "jest": {
    "preset": "expo-module-scripts"
  },
  "devDependencies": {
    "expo-module-scripts": "^3.0.0",
    "text-encoding": "^0.7.0",
    "whatwg-fetch": "^3.6.19",
    "base-64": "^1.0.0",
    "web-streams-polyfill": "^3.2.1"
  },
  "peerDependencies": {
    "expo": "*"
  }
}
